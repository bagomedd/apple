{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@components/": [
        "/Components/"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@icons/*": [
        "src/icons/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@svg/": [
        "src/svg/"
      ],
      "@fonts/": [
        "src/fonts/"
      ],
      "@pages/": [
        "src/pages/"
      ],
    }
  }
}